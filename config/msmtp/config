account mailbox
protocol smtp
host smtp.mailbox.org
port 465
auth on
tls on
tls_starttls off
tls_trust_file /etc/ssl/certs/ca-certificates.crt
eval "echo -n 'user '; pass show iz/www/mail/mailbox.org/username"
eval "echo -n 'from '; pass show iz/www/mail/mailbox.org/username"
passwordeval "pass show iz/www/mail/mailbox.org/password"

account disroot-prisoner
protocol smtp
host disroot.org
port 465
auth on
tls on
tls_starttls off
tls_trust_file /etc/ssl/certs/ca-certificates.crt
eval "printf 'user %s' `pass show iz/www/mail/disroot.org/prisoner/email`"
eval "printf 'from %s' `pass show iz/www/mail/disroot.org/prisoner/email`"
passwordeval "pass show iz/www/mail/disroot.org/prisoner/password"

