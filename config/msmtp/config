host disroot.org
port 465
auth on
tls on
tls_starttls off
tls_trust_file /etc/ssl/certs/ca-certificates.crt
eval "printf 'user %s' `pass show iz/www/mail/disroot.org/izemmouri/email`"
eval "printf 'from %s' `pass show iz/www/mail/disroot.org/izemmouri/email`"
passwordeval "pass show iz/www/mail/disroot.org/izemmouri/password"

