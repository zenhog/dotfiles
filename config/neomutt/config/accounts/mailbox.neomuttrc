set sendmail = "/usr/bin/msmtp -a mailbox"
set mbox_type = Maildir
set editor = nvim
set folder = "~/.mail/mailbox"
set header_cache = "~/.cache/neomutt"
set spoolfile = +INBOX
set postpone = yes
set postponed = +Drafts
set record = +Sent
set trash = +Trash
set maildir_trash = yes
set from = `pass show www/mail/mailbox.org/username`
